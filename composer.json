{
    "name": "maadhih/reinforcement",
    "description": "Extended laravel support for APIs",
    "keywords": ["extended", "API", "laravel", "support"],
    "homepage": "https://gitlab.com/maadhih/reinforcement",
    "license": "MIT",
    "authors": [{
        "name": "Maadhih Hussain",
        "email": "maadhihhussain@gmail.com"
    }],
    "require": {
        "php": ">=7.0",
        "illuminate/validation": "5.7.*",
        "illuminate/support": "5.7.*",
        "illuminate/container": "5.7.*",
        "illuminate/config": "5.7.*",
        "illuminate/database": "5.7.*",
        "illuminate/console": "5.7.*",
        "danielstjules/stringy": "~3.1.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Reinforcement\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Reinforcement\\ReinforcementServiceProvider",
                "Reinforcement\\ValidationServiceProvider"
            ]
        }
    }
}