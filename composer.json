{
    "name": "acnox/reinforcement",
    "description": "Extended laravel support for APIs",
    "keywords": ["extended", "API", "laravel", "support"],
    "homepage": "https://gitlab.com/Acnox/Reinforcement",
    "license": "MIT",
    "authors": [{
        "name": "Maadhih Hussain",
        "email": "maadhihhussain@gmail.com"
    }],
    "require": {
        "php": ">=5.6.0",
        "illuminate/validation": "5.*",
        "illuminate/support": "5.*",
        "illuminate/container": "5.*",
        "illuminate/config": "5.*",
        "illuminate/database": "5.*",
        "illuminate/console": "5.*",
        "danielstjules/stringy": "~3.1.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Reinforcement\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": ["Reinforcement\\ReinforcementServiceProvider"],
            "aliases": {
                "Schema": "Reinforcement\\Database\\Schema\\Facades\\Schema"
            }
        }
    }
}